package view.testView{	import actions.FREEZE;	import flash.display.SimpleButton;	import flash.text.TextField;	import flash.utils.getTimer;	import models.IMyModel;	import tl.viewController.ViewController;	public class AnotherTestViewController extends ViewController	{		[Injection]		public var myModel : IMyModel;		[Outlet]		public var myLabel : TextField;		[Outlet]		public var myButton : SimpleButton;		[Event(name="enterFrame")]		public function onEnterFrame() : void		{			myLabel.text = getTimer().toString( 2 );		}		[Event(name="myButtonClicked")]		public function myButtonClickedHandler() : void		{			myButton.mouseEnabled = false;			unsetHandler( "enterFrame" );			myLabel.text = myModel.myText;			actionDispatcher.dispatch( FREEZE );		}	}}