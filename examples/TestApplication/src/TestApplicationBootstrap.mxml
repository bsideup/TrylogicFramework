<?xml version="1.0"?>
<tl:Bootstrap xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:tl="http://www.trylogic.ru/" xmlns:service="service.*" xmlns:applicationview="view.applicationView.*"
			  xmlns:ioc="http://www.trylogic.ru/ioc/" xmlns:local="*">

	<tl:iocMap>
		<ioc:Associate iface="tl.actions.IActionLogger" withClass="core.ActionLogger" factory="tl.factory.SingletonFactory" />
		<ioc:Associate iface="core.ILogger" withClass="core.Logger" />
	</tl:iocMap>

	<tl:subModules>
		<local:TestViewBootstrap />
	</tl:subModules>

	<tl:services>
		<service:FreezeService text="onFreeze" />
	</tl:services>

	<tl:applicationView>
		<applicationview:ApplicationView controllerClass="view.applicationView.TestApplicationViewController" />
	</tl:applicationView>
</tl:Bootstrap>