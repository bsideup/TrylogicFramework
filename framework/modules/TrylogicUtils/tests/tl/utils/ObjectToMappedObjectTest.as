package tl.utils{	import org.flexunit.asserts.assertEquals;	import tl.utils.objectToMappedObjectTest.MyObject;	public class ObjectToMappedObjectTest	{		private static const obj : Object =		{			_metaClassName : "test.MyObject",			myString : "Yo!",			myNumber : 42.0,			myObj1 : {q : "QQQQQ"},			myArray : [				{ someMyString : "AAAAEEEE!" },				{ someMyString : "Yoyoyo!" },				{ someMyObject : { myNumber : 42 } }			],			myArrayWithInts : [1, 2, 3],			myAnotherObject : {				someMyString : "Fuck yeah!"			}		}				;		private static var instance : MyObject = ObjectToMappedObject.parseObject( obj, MyObject ) as MyObject;		[Test]		public function testSimpleProperties() : void		{			assertEquals( "Yo!", instance.myString );			assertEquals( 42, instance.myNumber );		}		[Test]		public function testComplexProperties() : void		{			assertEquals( "QQQQQ", instance.myObj1.q );			assertEquals( "Fuck yeah!", instance.myAnotherObject.someMyString );		}		[Test]		public function testArrays() : void		{			assertEquals( "AAAAEEEE!", instance.myArray[0].someMyString );			assertEquals( "Yoyoyo!", instance.myArray[1].someMyString );			assertEquals( 42, instance.myArray[2].someMyObject.myNumber );			assertEquals( 1, instance.myArrayWithInts[0] );			assertEquals( 2, instance.myArrayWithInts[1] );			assertEquals( 3, instance.myArrayWithInts[2] );		}	}}