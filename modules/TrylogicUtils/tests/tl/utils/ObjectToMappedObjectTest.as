package tl.utils{	import org.flexunit.asserts.assertEquals;	import tl.utils.objectToMappedObjectTest.MyObject;	public class ObjectToMappedObjectTest	{		private static const obj : Object =			{				_metaClassName : "test.MyObject",				myString : "Yo!",				myNumber : 42.0,				myObj1	 : {q : "QQQQQ"},				myArray :						[							{ someMyString : "AAAAEEEE!" },							{ someMyString : "Yoyoyo!" },							{ someMyObject : { myNumber : 42 } }						],				myAnotherObject : { someMyString : "Fuck yeah!" }			};		private static var instance : MyObject = ObjectToMappedObject.parseObject( obj, MyObject ) as MyObject;		[Test]		public function testSimpleProperties() : void		{			assertEquals( instance.myString, "Yo!" );			assertEquals( instance.myNumber, 42 );		}		[Test]		public function testComplexProperties() : void		{			assertEquals( instance.myObj1.q, "QQQQQ" );			assertEquals( instance.myAnotherObject.someMyString, "Fuck yeah!" );		}		[Test]		public function testArrays() : void		{			assertEquals( instance.myArray[0].someMyString, "AAAAEEEE!" );			assertEquals( instance.myArray[1].someMyString, "Yoyoyo!" );			assertEquals( instance.myArray[2].someMyObject.myNumber, 42 );		}	}}