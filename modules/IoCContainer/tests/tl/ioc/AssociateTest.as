package tl.ioc{	import flash.display.IBitmapDrawable;	import flash.display.Sprite;	import org.flexunit.asserts.assertEquals;	import tl.factory.SingletonFactory;	import tl.ioc.mxml.Associate;	public class AssociateTest	{		[Test]		public function testEquals() : void		{			var assoc : Associate = new Associate(IBitmapDrawable, Sprite, SingletonFactory);			assertEquals(assoc.iface, IBitmapDrawable);			assertEquals(assoc.withClass, Sprite);			assertEquals(assoc.factory, SingletonFactory);		}	}}