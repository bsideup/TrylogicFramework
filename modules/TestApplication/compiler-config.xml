<flex-config xmlns="http://www.adobe.com/2006/flex-config">

	<compiler>

		<debug>true</debug>
		<verbose-stacktraces>true</verbose-stacktraces>

		<incremental>false</incremental>

		<library-path append="true">
			<path-element>libs</path-element>
		</library-path>

		<include-libraries append="true">
			<library>./src/view/testView/TestView.swc</library>
		</include-libraries>

		<keep-generated-actionscript>true</keep-generated-actionscript>

		<use-resource-bundle-metadata>false</use-resource-bundle-metadata>

		<source-path>
			<path-element>./src</path-element>
		</source-path>

		<allow-source-path-overlap>true</allow-source-path-overlap>

	</compiler>

	<output>./out/TestApplication.swf</output>

</flex-config>