		<ioc:Associate iface="tl.actions.IActionLogger" withClass="controllers.ActionLogger" />
		<ioc:Associate iface="controllers.ILogger" withClass="controllers.Logger" />

		<ioc:Associate iface="views.testView.ITestViewController" withClass="viewControllers.TestViewController" />

		<ioc:Associate iface="models.IMyModel" withClass="models.MyModel" />

		<ioc:Associate iface="views.testView.ITestView" withClass="views.testView.TestView" />
