package tl.factory{	import flash.utils.Dictionary;	import tl.service.IService;	public class ServiceFactory	{		private static const services : Dictionary = new Dictionary();		public static function makeInstance( type : Class ) : Object		{			return services[type];		}		public static function registerService( type : Class, serviceInstance : IService ) : void		{			services[type] = serviceInstance;		}	}}